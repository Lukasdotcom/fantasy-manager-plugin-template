{
  "name": "fantasy-manager-plugin-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "pretty": "prettier --check .",
    "format": "prettier --write .",
    "test": "tsc -p tsconfig.json && npm run lint && npm run pretty"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.12",
    "prettier": "3.0.2",
    "typescript": "5.2.2"
  },
  "imports": {
    "#/*": "./*.js",
    "#type/*": "./types/*.js",
    "#Modules/*": "./Modules/*.js"
  }
}
