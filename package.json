{
  "name": "fantasy-manager-plugin-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "pretty": "prettier --check .",
    "format": "prettier --write .",
    "test": "tsc -p tsconfig.json && npm run lint && npm run pretty",
    "start": "ts-node --project=tsconfig.json ./main.ts"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "7.2.0",
    "@typescript-eslint/parser": "7.2.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.4.2"
  },
  "imports": {
    "#/*": "./*.js",
    "#type/*": "./types/*.js",
    "#Modules/*": "./Modules/*.js"
  }
}
