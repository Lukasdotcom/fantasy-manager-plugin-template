{
  "name": "fantasy-manager-plugin-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "pretty": "prettier --check .",
    "format": "prettier --write .",
    "test": "tsc -p tsconfig.json && npm run lint && npm run pretty"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "imports": {
    "#/*": "./*.js",
    "#type/*": "./types/*.js",
    "#Modules/*": "./Modules/*.js"
  }
}
